#include <iostream>
#include <vector>

#include <bits/stdc++.h>

using namespace std;

int main()
{
  vector <int> g1;
  int x;
  for(int i=1;i<=5;i++)
  {
      cin>>x;
     g1.push_back(x);
   }
   cout<<" array:"<<endl;
   for(auto i = g1.begin();i!=g1.end();i++)
   {
        cout<<*i<<" ";
    }
   cout<< " \nreverse form"<<endl;
  for(auto i = g1.rbegin();i!=g1.rend();i++)
   {
        cout<<*i<<" ";
    }
    cout<<"\n min Array "<<*min_element(g1.begin(),g1.end())<<endl;
    cout<<" max Array "<<*max_element(g1.begin(),g1.end())<<endl;
   return 0;
 }
